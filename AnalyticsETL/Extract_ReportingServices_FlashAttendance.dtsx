			Dts.TaskResult = (int)ScriptResults.Success;

            string url = "http://uatanalytics/ReportServer?" + System.Web.HttpUtility.UrlEncode(Dts.Variables["ItemPath"].Value.ToString()) + "&rs:Command=Render&rs:Format=EXCEL";

            SaveFile(url, Dts.Variables["OutputPath"].Value.ToString());

		}

        public void SaveFile (string url, string outputpath) {

            
            System.IO.Stream ResponseStream;
            System.IO.FileStream FileStream = new System.IO.FileStream(outputpath, System.IO.FileMode.Create, System.IO.FileAccess.Write);
            //Byte[] Bytes = new Byte[256];
            var byteArray = new Byte[256];
            int Count = 1;

            try { 
                
                System.Net.HttpWebRequest Request = (System.Net.HttpWebRequest) System.Net.WebRequest.Create(url);
                Request.Credentials = System.Net.CredentialCache.DefaultCredentials;
                //Request.Credentials = new System.Net.NetworkCredential("HBCReportReader", "R3p0rts*","UATAnalytics");
               
                Request.Timeout = 6000000;
                Request.Method = "GET";

                System.Net.WebResponse Response = Request.GetResponse();
                ResponseStream = Response.GetResponseStream();
                while (Count > 0) {
                    Count = ResponseStream.Read(byteArray, 0, 256);
                    FileStream.Write(byteArray, 0, Count);
                }
                FileStream.Flush();
                FileStream.Close();
            
            }
            catch (Exception ex) {
              Dts.Events.FireError(0, "Error Reading from SSRS", ex.Message, string.Empty, 0);
            }



        }

        

        #region ScriptResults declaration
