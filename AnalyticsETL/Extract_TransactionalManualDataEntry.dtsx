      <DTS:Variables />
      <DTS:PropertyExpression
        DTS:Name="SqlStatementSource">"SELECT TenantID, Source AS ConnectionString 
FROM TenantEtlProcess tep
INNER JOIN ETLProcess ep
    ON tep.ETLProcessID = ep.ETLProcessID
INNER JOIN ConnectionString cs
	ON tep.ConnectionStringID = cs.ConnectionStringID 
WHERE
    ep.Name = '" + @[User::_DestinationTableName]  + "';"</DTS:PropertyExpression>
