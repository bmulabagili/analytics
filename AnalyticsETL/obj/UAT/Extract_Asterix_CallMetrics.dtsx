                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
	  CalendarYear
	, CalendarMonth
	, SUM(ResourceAndDonationInfocision) AS DonationCalls
	, SUM(PrayerHandled) AS PrayerCalls
FROM (
	--donations call
	SELECT  
  		  YEAR(cdr.calldate) AS CalendarYear
		, MONTH(cdr.calldate) AS CalendarMonth
		, COUNT(1) AS ResourceAndDonationInfocision
		, 0 AS PrayerHandled
	FROM cdr
	LEFT JOIN cdrprocessed cdrp 
		ON cdrp.id = cdr.id
	WHERE 
		(cdr.userfield LIKE 'inbound-8885819673%' 
		OR cdr.userfield LIKE 'inbound-8005456800%' 
		OR cdr.userfield LIKE 'inbound-8003210131%')  
		and lastapp NOT LIKE 'ForkCDR'
		and cdrp.displayeddst = '4011@agents'
		AND  DATENAME(WEEKDAY, cdr.calldate) NOT IN ('Saturday','Sunday')
		AND DATEPART(HOUR, cdr.CallDate)  BETWEEN 7 and 10
	GROUP BY 
		  YEAR(cdr.calldate)
		, MONTH(cdr.calldate)
	UNION
	--prayers
	SELECT  
		  YEAR(cdr.calldate) AS CalendarYear
		, MONTH(cdr.calldate) AS CalendarMonth
		, 0 AS DonationCalls
		, COUNT(1)  AS PrayerCalls
	FROM cdr
	LEFT JOIN cdrprocessed cdrp 
		ON cdrp.id = cdr.id
	WHERE 
		(cdr.userfield LIKE 'inbound-8885819673%' 
		OR cdr.userfield LIKE 'inbound-8005456800%' 
		OR cdr.userfield LIKE 'inbound-8003210131%')  
		and lastapp NOT LIKE 'ForkCDR'
		and cdrp.displayeddst in ('4012@agents','4013@agents', '4014@agents','4019@agents','4020@agents', '4021@agents','4022@agents')
		AND  DATENAME(WEEKDAY, cdr.calldate) NOT IN ('Saturday','Sunday')
		AND DATEPART(HOUR, cdr.CallDate)  BETWEEN 7 and 10
	GROUP BY 
		  YEAR(cdr.calldate)
		, MONTH(cdr.calldate)
)src
GROUP BY
	  CalendarYear
	, CalendarMonth</property>
