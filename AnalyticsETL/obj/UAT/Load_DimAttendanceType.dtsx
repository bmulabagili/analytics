        {
            Variables vars = null;
            bool isUnlocked = false;
            object result;

            try
            {
                Dts.VariableDispenser.LockForRead(variableName);
                Dts.VariableDispenser.GetVariables(ref vars);

                try
                {
                    result = vars[variableName].Value;
                }
                finally
                {
                    if (vars != null)
                    {
                        vars.Unlock();
                        isUnlocked = true;
                    }
                }
            }
            catch (Exception)
            {
