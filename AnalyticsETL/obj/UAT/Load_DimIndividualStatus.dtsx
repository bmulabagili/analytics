                bool bLogVar = true;
                string sVarList = ReadVariable("User::varSSISOps_VariableValues").ToString();

                if (string.IsNullOrEmpty(sVarList) || Convert.ToBoolean(ReadVariable("User::varSSISOps_DisableConnLogging")))
