			, CASE ISNULL( CampusAllocation.Code, CampusXLTReportGroup_CampusTab.CampusCode)
				WHEN 'Initiatives/BB' THEN 1
				WHEN 'EL' THEN 2
				WHEN 'RM' THEN 3
