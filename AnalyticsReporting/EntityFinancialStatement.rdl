	SELECT DISTINCT 'HBF' EntityCode, GroupID, 0 Amount FROM GLReportGrouping WHERE Category ='Revenue'
	UNION
	SELECT DISTINCT 'HCA' EntityCode, GroupID, 0 Amount FROM GLReportGrouping WHERE Category ='Revenue'
