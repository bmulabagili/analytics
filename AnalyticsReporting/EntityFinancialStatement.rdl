	FROM dw.FactBudgetExpense
	INNER JOIN dw.DimEntity
		ON FactBudgetExpense.EntityID = DimEntity.EntityID
		AND FactBudgetExpense.TenantID = DimEntity.TenantID
	INNER JOIN dw.DimFinancialCategory
		ON FactBudgetExpense.FinancialCategoryID = DimFinancialCategory.FinancialCategoryID
		AND FactBudgetExpense.TenantID = DimFinancialCategory.TenantID
	LEFT JOIN GLReportGrouping glrg 
		ON DimFinancialCategory.GLCode = glrg.GLCode
		AND glrg.Category = 'Expense'
		AND DimEntity.Code = glrg.EntityCode 
	WHERE
		FactBudgetExpense.BudgetMonth = @CalendarMonth
		AND FactBudgetExpense.BudgetYear = @CalendarYear
		AND (
			DimEntity.Code IN ('HBF','HCA','WITW')
			OR --HBC only reports fund 025
			(DimEntity.Code = 'HBC' AND DimFinancialCategory.FundCode = '025')
		)
	GROUP BY
		DimEntity.Code
		, ISNULL(GroupID, 8)
	UNION
	SELECT DISTINCT 'HBF' EntityCode, GroupID, 0 Amount FROM GLReportGrouping
	UNION
	SELECT DISTINCT 'HCA' EntityCode, GroupID, 0 Amount FROM GLReportGrouping
	UNION
	SELECT DISTINCT 'HBC' EntityCode, GroupID, 0 Amount FROM GLReportGrouping
	UNION
	SELECT DISTINCT 'WITW' EntityCode, GroupID, 0 Amount FROM GLReportGrouping
), ExpenseVariance AS (
	SELECT 
		  ISNULL(ExpenseActuals.EntityCode, ExpenseBudgeted.EntityCode) AS EntityCode
		, SUM(ISNULL(ExpenseBudgeted.Amount, 0)) - SUM(ISNULL(ExpenseActuals.Amount, 0)) AS Amount
		
	FROM ExpenseActuals
	FULL OUTER JOIN ExpenseBudgeted
		ON ExpenseActuals.EntityCode = ExpenseBudgeted.EntityCode
		AND ExpenseActuals.GroupID = ExpenseBudgeted.GroupID
	--WHERE
		--ISNULL(ExpenseActuals.EntityCode, ExpenseBudgeted.EntityCode) IS NOT NULL
	GROUP BY
		  ISNULL(ExpenseActuals.EntityCode, ExpenseBudgeted.EntityCode)
	UNION SELECT 'HBF' , 0
	UNION SELECT 'HCA' , 0
	UNION SELECT 'HBC' , 0
	UNION SELECT 'WITW', 0
)

SELECT 
	EntityCode
	, SUM(Amount) AS Amount
FROM (
	select EntityCode, sum(amount) AS Amount from RevenueVariance group by EntityCode
	UNION
	select EntityCode, sum(amount * -1) from ExpenseVariance group by EntityCode)x
GROUP BY
	EntityCode</CommandText>
