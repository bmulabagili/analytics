		AND DimDate.CalendarYear = @CalendarYear
		AND DimDate.ActualDate &lt;= GETDATE() --possible parameter3
		AND (
			DimEntity.Code IN ('HBF','HCA','WITW')
			OR --HBC only reports fund 025
			(DimEntity.Code = 'HBC' AND DimFinancialCategory.FundCode = '025')
		)
		--AND DimFinancialCategory.GLType = 'REV'
	GROUP BY
		  DimEntity.Code
		, ISNULL(GroupID, 8)
	UNION
	SELECT DISTINCT 'HBF' EntityCode, GroupID, 0 Amount FROM GLReportGrouping
	UNION
	SELECT DISTINCT 'HCA' EntityCode, GroupID, 0 Amount FROM GLReportGrouping
	UNION
	SELECT DISTINCT 'HBC' EntityCode, GroupID, 0 Amount FROM GLReportGrouping
	UNION
	SELECT DISTINCT 'WITW' EntityCode, GroupID, 0 Amount FROM GLReportGrouping
), ExpenseBudgeted AS (
	SELECT 
		  DimEntity.Code AS EntityCode
