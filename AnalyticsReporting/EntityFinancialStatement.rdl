	FULL OUTER JOIN BudgetedRevenue
		ON ActualRevenue.EntityCode = BudgetedRevenue.EntityCode
		AND ActualRevenue.GroupID = BudgetedRevenue.GroupID
	GROUP BY
			ISNULL(ActualRevenue.EntityCode, BudgetedRevenue.EntityCode)
		, ActualRevenue.GroupID
), ActualExpenses AS (
	SELECT 
		 CASE DimFinancialCategory.FundCode 
			WHEN '055' THEN 'BSC'
			WHEN '058' THEN 'Camp'
			WHEN '069' THEN 'VCF'
			WHEN '078' THEN 'Curriculum'
			WHEN '098' THEN 'VCB'
		  END AS EntityCode
		, ISNULL(GroupID, 8) AS GroupID
		, SUM(FactExpense.Amount) AS Amount
	FROM dw.FactExpense
	INNER JOIN dw.DimDate
		ON FactExpense.DateID = DimDate.DateID
	INNER JOIN dw.DimEntity
		ON FactExpense.EntityID = DimEntity.EntityID
		AND FactExpense.TenantID = DimEntity.TenantID
	INNER JOIN dw.DimFinancialCategory
		ON FactExpense.FinancialCategoryID = DimFinancialCategory.FinancialCategoryID
		AND FactExpense.TenantID = DimFinancialCategory.TenantID
	LEFT JOIN GLReportGrouping glrg --Check out the ones that DON'T map!
		ON DimFinancialCategory.GLCode = glrg.GLCode
		AND glrg.Category = 'Expense'
		AND DimEntity.Code = glrg.EntityCode 
	WHERE
		DimDate.CalendarYear = @CalendarYear
		AND DimDate.ActualDate &lt;= DATEADD(DAY, -1, DATEADD(MONTH, 1, CONVERT(DATE, CONVERT(VARCHAR(2), @CalendarMonth) + '/1/' + CONVERT(VARCHAR(4), @CalendarYear))))
		AND DimFinancialCategory.EntityCode = 'HBC'
		AND DimFinancialCategory.FundCode IN('055','058','069','078','098')
	GROUP BY
		DimEntity.Code
		, ISNULL(GroupID, 8)
		, CASE DimFinancialCategory.FundCode 
			WHEN '055' THEN 'BSC'
			WHEN '058' THEN 'Camp'
			WHEN '069' THEN 'VCF'
			WHEN '078' THEN 'Curriculum'
			WHEN '098' THEN 'VCB'
		  END
	UNION
	SELECT DISTINCT 'Camp' EntityCode, GroupID, 0 Amount FROM GLReportGrouping WHERE Category ='Expense'
	UNION
	SELECT DISTINCT 'VCF' EntityCode, GroupID, 0 Amount FROM GLReportGrouping WHERE Category ='Expense'
	UNION
	SELECT DISTINCT 'VCB' EntityCode, GroupID, 0 Amount FROM GLReportGrouping WHERE Category ='Expense'
	UNION
	SELECT DISTINCT 'BSC' EntityCode, GroupID, 0 Amount FROM GLReportGrouping WHERE Category ='Expense'
	UNION
	SELECT DISTINCT 'Curriculum' EntityCode, GroupID, 0 Amount FROM GLReportGrouping WHERE Category ='Expense'
), BudgetedExpenses AS (
	SELECT 
		 CASE DimFinancialCategory.FundCode 
			WHEN '055' THEN 'BSC'
			WHEN '058' THEN 'Camp'
			WHEN '069' THEN 'VCF'
			WHEN '078' THEN 'Curriculum'
			WHEN '098' THEN 'VCB'
		  END AS EntityCode
		, ISNULL(GroupID, 8) AS GroupID
		, SUM(FactBudgetExpense.Amount) AS Amount
	FROM dw.FactBudgetExpense
	INNER JOIN dw.DimEntity
		ON FactBudgetExpense.EntityID = DimEntity.EntityID
		AND FactBudgetExpense.TenantID = DimEntity.TenantID
	INNER JOIN dw.DimFinancialCategory
		ON FactBudgetExpense.FinancialCategoryID = DimFinancialCategory.FinancialCategoryID
		AND FactBudgetExpense.TenantID = DimFinancialCategory.TenantID
	LEFT JOIN GLReportGrouping glrg 
		ON DimFinancialCategory.GLCode = glrg.GLCode
		AND glrg.Category = 'Expense'
		AND DimEntity.Code = glrg.EntityCode 
	WHERE
		FactBudgetExpense.BudgetYear = @CalendarYear
		AND FactBudgetExpense.BudgetMonth &lt;= @CalendarMonth
		AND DimFinancialCategory.EntityCode = 'HBC'
		AND DimFinancialCategory.FundCode IN('055','058','069','078','098')
	GROUP BY
		DimEntity.Code
		, ISNULL(GroupID, 8)
		, CASE DimFinancialCategory.FundCode 
			WHEN '055' THEN 'BSC'
			WHEN '058' THEN 'Camp'
			WHEN '069' THEN 'VCF'
			WHEN '078' THEN 'Curriculum'
			WHEN '098' THEN 'VCB'
		  END
	UNION
	SELECT DISTINCT 'Camp' EntityCode, GroupID, 0 Amount FROM GLReportGrouping WHERE Category ='Expense'
	UNION
	SELECT DISTINCT 'VCF' EntityCode, GroupID, 0 Amount FROM GLReportGrouping WHERE Category ='Expense'
	UNION
	SELECT DISTINCT 'VCB' EntityCode, GroupID, 0 Amount FROM GLReportGrouping WHERE Category ='Expense'
	UNION
	SELECT DISTINCT 'BSC' EntityCode, GroupID, 0 Amount FROM GLReportGrouping WHERE Category ='Expense'
	UNION
	SELECT DISTINCT 'Curriculum' EntityCode, GroupID, 0 Amount FROM GLReportGrouping WHERE Category ='Expense'
), VarianceExpenses as (
	SELECT 
		  ISNULL(ActualExpenses.EntityCode, BudgetedExpenses.EntityCode) AS EntityCode
		, ActualExpenses.GroupID
		, SUM(ISNULL(BudgetedExpenses.Amount, 0)) - SUM(ISNULL(ActualExpenses.Amount, 0)) AS Amount
	FROM ActualExpenses
	FULL OUTER JOIN BudgetedExpenses
		ON ActualExpenses.EntityCode = BudgetedExpenses.EntityCode
		AND ActualExpenses.GroupID = BudgetedExpenses.GroupID
	GROUP BY
		  ISNULL(ActualExpenses.EntityCode, BudgetedExpenses.EntityCode)
		, ActualExpenses.GroupID
)
SELECT 
	EntityCode
	, SUM(Amount) AS Amount
FROM (
	select EntityCode, sum(amount) AS Amount from VarianceRevenue group by EntityCode
	UNION
	select EntityCode, sum(amount * -1) from VarianceExpenses group by EntityCode)x
GROUP BY
	EntityCode</CommandText>
