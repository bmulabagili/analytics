	UNION
	SELECT DISTINCT 'WITW' EntityCode, GroupID, 0 Amount FROM GLReportGrouping WHERE Category ='Revenue'
), RevenueVariance AS (
	SELECT 
		  ISNULL(RevenueActuals.EntityCode, RevenueBudgeted.EntityCode) AS EntityCode
		, SUM(ISNULL(RevenueBudgeted.Amount, 0)) - SUM(ISNULL(RevenueActuals.Amount, 0)) AS Amount
		
	FROM RevenueActuals
	FULL OUTER JOIN RevenueBudgeted
		ON RevenueActuals.EntityCode = RevenueBudgeted.EntityCode
		AND RevenueActuals.GroupID = RevenueBudgeted.GroupID
