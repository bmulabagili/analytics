  </DataSources>
  <DataSets>
    <DataSet Name="Attendance">
      <Query>
        <DataSourceName>DW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CalendarYear">
            <Value>=Parameters!CalendarYear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CalendarMonth">
            <Value>=Parameters!CalendarMonth.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
SELECT 
	  DimDate.CalendarYear
	, DimDate.CalendarMonth
	, DimDate.CalendarMonthAbbreviation
	, CASE WHEN DimActivity.Name LIKE '%Worship%' THEN 'Worship Attendance - Adults'
		ELSE CASE WHEN DimMinistry.Name LIKE '%Harvest Kids%' THEN 'Worship Attenance - Children' END 
	  END AS AttendanceCategory
	, FactAttendance.CampusID
	, DimCampus.Code
	, SUM(FactAttendance.AttendanceCount) AS AttendanceCount
FROM DW.FactAttendance
INNER JOIN DW.DimDate
	ON FactAttendance.InstanceDateID = DimDate.DateID
INNER JOIN DW.DimCampus
	ON FactAttendance.CampusID = DimCampus.CampusID
	AND FactAttendance.TenantID = DimCampus.TenantID
INNER JOIN DW.DimMinistry
	ON FactAttendance.MinistryID = DimMinistry.MinistryID
	AND FactAttendance.TenantID = DimMinistry.TenantID
--may not have to include activity, and attendanceType
INNER JOIN DW.DimActivity 
	ON FactAttendance.ActivityID = dimActivity.ActivityID
	AND FactAttendance.TenantID = DimActivity.TenantID
--INNER JOIN DW.DimRoster -- adds too much time in this query.
--	ON FactAttendance.RosterID = DimRoster.RosterID
--	AND FactAttendance.TenantID = DimRoster.TenantID
INNER JOIN DW.DimAttendanceType
	ON FactAttendance.AttendanceTypeID = DimAttendanceType.AttendanceTypeID
	AND FactAttendance.TenantID = DimAttendanceType.TenantID
WHERE
	DimDate.DateID BETWEEN ((@CalendarYear - 1) * 10000) + 101 AND ((@CalendarYear) * 10000) + ((@CalendarMonth + 1) * 100)
	AND DimAttendanceType.Category = 'Attendee'
	AND CASE WHEN DimActivity.Name LIKE '%Worship%' THEN 'Worship Attendance - Adults'
		ELSE CASE WHEN DimMinistry.Name LIKE '%Harvest Kids%' THEN 'Worship Attenance - Children' END 
	  END IS NOT NULL
GROUP BY
	  DimDate.CalendarYear
	, DimDate.CalendarMonth
	, DimDate.CalendarMonthAbbreviation
	, CASE WHEN DimActivity.Name LIKE '%Worship%' THEN 'Worship Attendance - Adults'
		ELSE CASE WHEN DimMinistry.Name LIKE '%Harvest Kids%' THEN 'Worship Attenance - Children' END 
	  END
	, FactAttendance.CampusID
	, DimCampus.Code
	, DimMinistry.CampusID
</CommandText>
      </Query>
